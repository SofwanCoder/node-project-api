{
  "name": "project",
  "version": "1.0.0",
  "description": "PROJECT API",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "start": "node dist/index.js",
    "dev": "nodemon -x ts-node src/index.ts",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "test": "jest ",
    "lint": "eslint -c .eslintrc.js",
    "doc": "tsoa spec"
  },
  "types": "src/index.ts",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "testEnvironment": "node",
        "tsconfig": "tsconfig.json"
      }
    },
    "modulePaths": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/tests/*.+(ts|tsx)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SofwanCoder/project_api"
  },
  "keywords": [
    "project",
    "api"
  ],
  "author": "Sofwancoder",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SofwanCoder/project_api/issues"
  },
  "homepage": "https://github.com/SofwanCoder/project_api#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bignumber.js": "^9.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "faker": "^5.5.3",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "js-base64": "^3.6.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.6.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.4",
    "sequelize-typescript": "^2.1.0",
    "socket.io": "^4.1.2",
    "strong-error-handler": "^4.0.0",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.14.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.16",
    "@types/coinbase-commerce-node": "^1.0.5",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/method-override": "^0.0.31",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.6",
    "@types/node": "^14.17.2",
    "@types/nodemailer": "^6.4.2",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.6.2",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "chai": "^4.3.4",
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^27.0.6",
    "nodemon": "^2.0.9",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typedoc": "^0.21.2",
    "typedoc-plugin-markdown": "^3.10.2",
    "typescript": "^4.3.5"
  }
}
