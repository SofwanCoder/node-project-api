{
  "name": "project",
  "version": "0.0.1",
  "description": "PROJECT API",
  "main": "dist/index.js",
  "engines": {
    "node": "^16.x",
    "yarn": "^1.x"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc",
    "build": "yarn clean && yarn doc  && yarn compile",
    "start": "node dist/index.js",
    "dev": "nodemon -x ts-node src/index.ts",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "test": "jest ",
    "lint": "eslint -c .eslintrc.js",
    "doc": "tsoa spec",
    "heroku-postbuild": "yarn build && yarn knex migrate:latest"
  },
  "types": "src/index.ts",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "testEnvironment": "node",
        "tsconfig": "tsconfig.json"
      }
    },
    "modulePaths": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/tests/*.+(ts|tsx)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SofwanCoder/project_api"
  },
  "keywords": [
    "project",
    "api"
  ],
  "author": "Sofwancoder",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SofwanCoder/project_api/issues"
  },
  "homepage": "https://github.com/SofwanCoder/project_api#readme",
  "dependencies": {
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "bignumber.js": "^9.0.2",
    "body-parser": "^1.19.1",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "faker": "^5.5.3",
    "helmet": "^5.0.2",
    "http-status-codes": "^2.2.0",
    "js-base64": "^3.7.2",
    "jsonwebtoken": "^8.5.1",
    "juice": "^8.0.0",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql2": "^2.3.3",
    "node-cron": "^3.0.0",
    "node-graceful-shutdown": "^1.1.2",
    "nodemailer": "^6.7.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.16.1",
    "sequelize-typescript": "^2.1.2",
    "slugify": "^1.6.5",
    "socket.io": "^4.4.1",
    "strong-error-handler": "^4.0.0",
    "supertest": "^6.2.2",
    "swagger-ui-express": "^4.3.0",
    "tsoa": "^4.x",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.180",
    "@types/method-override": "^0.0.32",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.x",
    "@types/node-cron": "^3.0.1",
    "@types/nodemailer": "^6.4.4",
    "@types/sequelize": "^4.28.11",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chai": "^4.3.6",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
